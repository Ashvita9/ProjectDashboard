ved() {\r\n      this.$store.dispatch('projects/fetchProjects')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.projects-list {\r\n  background-color: #f8f9fa;\r\n  min-height: calc(100vh - 56px);\r\n}\r\n</style>\r\n"],"mappings":";AAkDA,SAAAA,UAAA;AACA,OAAAC,WAAA;AACA,OAAAC,gBAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ,WAAA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,eAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAR,UAAA;IACAS,SAAA;MACA,YAAAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,MAAA,CAAAC,QAAA;EACA;EACAC,OAAA;IACAC,gBAAA;MACA,KAAAR,eAAA;MACA,KAAAS,QAAA,CAAAC,IAAA;IACA;IACAC,cAAAC,OAAA;MACA,KAAAZ,eAAA;QAAA,GAAAY;MAAA;MACA,KAAAH,QAAA,CAAAC,IAAA;IACA;IACAG,YAAAD,OAAA;MACA,KAAAP,MAAA,CAAAC,QAAA,8BAAAM,OAAA,CAAAE,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,cAAAJ,OAAA,CAAAE,EAAA;IACA;IACAG,cAAAL,OAAA;MACA,KAAAH,QAAA,CAAAS,aAAA,CACA,oCAAAN,OAAA,CAAAf,IAAA,oCACA;QACAsB,KAAA;QACAC,IAAA;QACAC,UAAA;QACAC,SAAA;QACAC,OAAA;QACAC,WAAA;QACAC,QAAA;MACA,CACA,EAAAC,IAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAtB,MAAA,CAAAC,QAAA,2BAAAM,OAAA,CAAAE,EAAA;UACAlB,QAAA,CAAAgC,KAAA;YACAC,OAAA;YACAC,OAAA;UACA;QACA;MACA;IACA;IACAC,mBAAA;MACA,KAAA1B,MAAA,CAAAC,QAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}