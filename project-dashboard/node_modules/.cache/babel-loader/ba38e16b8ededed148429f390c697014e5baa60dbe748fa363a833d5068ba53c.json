   eventBus.$emit('show-notification', {\n          message: 'Registration failed. Please try again.',\n          variant: 'danger'\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["required","email","minLength","sameAs","mapActions","eventBus","name","data","form","username","password","confirmPassword","validations","sameAsPassword","methods","validateState","$dirty","$error","$v","handleRegister","$touch","$invalid","avatarUrl","register","avatar","$emit","message","variant","$router","push","error"],"sources":["src/views/Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"register-page\">\r\n    <!-- Animated Background -->\r\n    <div class=\"animated-bg\"></div>\r\n    \r\n    <b-container class=\"register-container\">\r\n      <b-row class=\"justify-content-center align-items-center min-vh-100\">\r\n        <b-col md=\"6\" lg=\"5\">\r\n          <div class=\"register-card\">\r\n            <div class=\"text-center mb-4\">\r\n              <h2 class=\"register-title\">Create Account</h2>\r\n              <p class=\"text-muted\">Join us to manage your projects</p>\r\n            </div>\r\n\r\n            <b-form @submit.prevent=\"handleRegister\">\r\n              <!-- Full Name -->\r\n              <b-form-group label=\"Full Name\" label-for=\"fullname\">\r\n                <b-form-input\r\n                  id=\"fullname\"\r\n                  v-model=\"form.name\"\r\n                  type=\"text\"\r\n                  placeholder=\"Enter your full name\"\r\n                  :state=\"validateState('name')\"\r\n                  @blur=\"$v.form.name.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback>\r\n                  Name is required\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <!-- Email -->\r\n              <b-form-group label=\"Email\" label-for=\"email\">\r\n                <b-form-input\r\n                  id=\"email\"\r\n                  v-model=\"form.email\"\r\n                  type=\"email\"\r\n                  placeholder=\"Enter your email\"\r\n                  :state=\"validateState('email')\"\r\n                  @blur=\"$v.form.email.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback>\r\n                  <span v-if=\"!$v.form.email.required\">Email is required</span>\r\n                  <span v-else-if=\"!$v.form.email.email\">Please enter a valid email</span>\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <!-- Username -->\r\n              <b-form-group label=\"Username\" label-for=\"username\">\r\n                <b-form-input\r\n                  id=\"username\"\r\n                  v-model=\"form.username\"\r\n                  type=\"text\"\r\n                  placeholder=\"Choose a username\"\r\n                  :state=\"validateState('username')\"\r\n                  @blur=\"$v.form.username.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback>\r\n                  Username must be at least 3 characters\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <!-- Password -->\r\n              <b-form-group label=\"Password\" label-for=\"password\">\r\n                <b-form-input\r\n                  id=\"password\"\r\n                  v-model=\"form.password\"\r\n                  type=\"password\"\r\n                  placeholder=\"Create a password\"\r\n                  :state=\"validateState('password')\"\r\n                  @blur=\"$v.form.password.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback>\r\n                  Password must be at least 6 characters\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <!-- Confirm Password -->\r\n              <b-form-group label=\"Confirm Password\" label-for=\"confirmPassword\">\r\n                <b-form-input\r\n                  id=\"confirmPassword\"\r\n                  v-model=\"form.confirmPassword\"\r\n                  type=\"password\"\r\n                  placeholder=\"Confirm your password\"\r\n                  :state=\"validateState('confirmPassword')\"\r\n                  @blur=\"$v.form.confirmPassword.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback>\r\n                  Passwords must match\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <!-- Submit Button -->\r\n              <b-button\r\n                type=\"submit\"\r\n                variant=\"primary\"\r\n                block\r\n                size=\"lg\"\r\n                class=\"register-btn mt-4\"\r\n                :disabled=\"$v.$invalid\"\r\n              >\r\n                Create Account\r\n              </b-button>\r\n\r\n              <!-- Login Link -->\r\n              <div class=\"text-center mt-3\">\r\n                <p class=\"text-muted\">\r\n                  Already have an account?\r\n                  <router-link to=\"/login\" class=\"login-link\">Sign In</router-link>\r\n                </p>\r\n              </div>\r\n            </b-form>\r\n          </div>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { required, email, minLength, sameAs } from 'vuelidate/lib/validators'\r\nimport { mapActions } from 'vuex'\r\nimport { eventBus } from '@/bus/eventBus'\r\n\r\nexport default {\r\n  name: 'Register',\r\n  \r\n  data() {\r\n    return {\r\n      form: {\r\n        name: '',\r\n        email: '',\r\n        username: '',\r\n        password: '',\r\n        confirmPassword: ''\r\n      }\r\n    }\r\n  },\r\n\r\n  validations: {\r\n    form: {\r\n      name: {\r\n        required\r\n      },\r\n      email: {\r\n        required,\r\n        email\r\n      },\r\n      username: {\r\n        required,\r\n        minLength: minLength(3)\r\n      },\r\n      password: {\r\n        required,\r\n        minLength: minLength(6)\r\n      },\r\n      confirmPassword: {\r\n        required,\r\n        sameAsPassword: sameAs('password')\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    ...mapActions('auth', ['register']),\r\n\r\n    /**\r\n     * Validate field state for Bootstrap Vue\r\n     */\r\n    validateState(name) {\r\n      const { $dirty, $error } = this.$v.form[name]\r\n      return $dirty ? !$error : null\r\n    },\r\n\r\n    /**\r\n     * Handle registration form submission\r\n     */\r\n    async handleRegister() {\r\n      this.$v.form.$touch()\r\n      if (this.$v.form.$invalid) {\r\n        return\r\n      }\r\n\r\n      try {\r\n        // Generate avatar URL using DiceBear API\r\n        const avatarUrl = `https://api.dicebear.com/7.x/avataaars/svg?seed=${this.form.username}`\r\n        \r\n        // Register user with avatar\r\n        await this.register({\r\n          name: this.form.name,\r\n          email: this.form.email,\r\n          username: this.form.username,\r\n          password: this.form.password,\r\n          avatar: avatarUrl\r\n        })\r\n\r\n        eventBus.$emit('show-notification', {\r\n          message: 'Account created successfully! Please login.',\r\n          variant: 'success'\r\n        })\r\n\r\n        // Redirect to login\r\n        this.$router.push('/login')\r\n      } catch (error) {\r\n        eventBus.$emit('show-notification', {\r\n          message: 'Registration failed. Please try again.',\r\n          variant: 'danger'\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.register-page {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  overflow: hidden;\r\n  background: linear-gradient(135deg, #D4C5F9 0%, #FFD1DC 50%, #B8D8F2 100%);\r\n}\r\n\r\n.animated-bg {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 0;\r\n}\r\n\r\n.animated-bg::before,\r\n.animated-bg::after {\r\n  content: '';\r\n  position: absolute;\r\n  border-radius: 50%;\r\n  animation: float 6s ease-in-out infinite;\r\n}\r\n\r\n.animated-bg::before {\r\n  width: 300px;\r\n  height: 300px;\r\n  background: radial-gradient(circle, rgba(255, 209, 220, 0.4), transparent);\r\n  top: 10%;\r\n  left: 10%;\r\n  animation-delay: 0s;\r\n}\r\n\r\n.animated-bg::after {\r\n  width: 400px;\r\n  height: 400px;\r\n  background: radial-gradient(circle, rgba(212, 197, 249, 0.4), transparent);\r\n  bottom: 10%;\r\n  right: 10%;\r\n  animation-delay: 3s;\r\n}\r\n\r\n@keyframes float {\r\n  0%, 100% {\r\n    transform: translateY(0) scale(1);\r\n  }\r\n  50% {\r\n    transform: translateY(-30px) scale(1.05);\r\n  }\r\n}\r\n\r\n.register-container {\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.register-card {\r\n  background: rgba(255, 255, 255, 0.95);\r\n  backdrop-filter: blur(10px);\r\n  border-radius: 24px;\r\n  padding: 2.5rem;\r\n  box-shadow: \r\n    8px 8px 24px rgba(212, 197, 249, 0.3),\r\n    -8px -8px 24px rgba(255, 255, 255, 0.9),\r\n    inset 2px 2px 8px rgba(255, 255, 255, 0.5);\r\n  border: 1px solid rgba(255, 255, 255, 0.5);\r\n  animation: fadeIn 0.6s ease-out;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n.register-title {\r\n  font-size: 2rem;\r\n  font-weight: 700;\r\n  background: linear-gradient(135deg, #D4C5F9, #B8D8F2);\r\n  background-clip: text;\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.register-btn {\r\n  background: linear-gradient(135deg, #D4C5F9, #B8D8F2);\r\n  border: none;\r\n  border-radius: 12px;\r\n  font-weight: 600;\r\n  transition: all 0.3s ease;\r\n  box-shadow: \r\n    4px 4px 12px rgba(212, 197, 249, 0.3),\r\n    -2px -2px 8px rgba(255, 255, 255, 0.8);\r\n}\r\n\r\n.register-btn:hover:not(:disabled) {\r\n  transform: translateY(-2px);\r\n  box-shadow: \r\n    6px 6px 16px rgba(212, 197, 249, 0.4),\r\n    -3px -3px 10px rgba(255, 255, 255, 0.9);\r\n}\r\n\r\n.register-btn:active:not(:disabled) {\r\n  transform: translateY(0);\r\n  box-shadow: \r\n    inset 2px 2px 8px rgba(212, 197, 249, 0.3),\r\n    inset -1px -1px 4px rgba(255, 255, 255, 0.5);\r\n}\r\n\r\n.login-link {\r\n  color: #D4C5F9;\r\n  font-weight: 600;\r\n  text-decoration: none;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.login-link:hover {\r\n  color: #B8D8F2;\r\n  text-decoration: underline;\r\n}\r\n\r\n.form-control {\r\n  border-radius: 12px;\r\n  border: 2px solid transparent;\r\n  background: rgba(255, 255, 255, 0.8);\r\n  box-shadow: \r\n    inset 2px 2px 6px rgba(212, 197, 249, 0.2),\r\n    inset -1px -1px 3px rgba(255, 255, 255, 0.5);\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.form-control:focus {\r\n  border-color: #D4C5F9;\r\n  background: rgba(255, 255, 255, 1);\r\n  box-shadow: \r\n    0 0 0 0.2rem rgba(212, 197, 249, 0.25),\r\n    inset 2px 2px 6px rgba(212, 197, 249, 0.15);\r\n}\r\n\r\n.form-label {\r\n  font-weight: 600;\r\n  color: #6c757d;\r\n  margin-bottom: 0.5rem;\r\n}\r\n</style>\r\n"],"mappings":";AAuHA,SAAAA,QAAA,EAAAC,KAAA,EAAAC,SAAA,EAAAC,MAAA;AACA,SAAAC,UAAA;AACA,SAAAC,QAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,IAAA;QACAF,IAAA;QACAL,KAAA;QACAQ,QAAA;QACAC,QAAA;QACAC,eAAA;MACA;IACA;EACA;EAEAC,WAAA;IACAJ,IAAA;MACAF,IAAA;QACAN;MACA;MACAC,KAAA;QACAD,QAAA;QACAC;MACA;MACAQ,QAAA;QACAT,QAAA;QACAE,SAAA,EAAAA,SAAA;MACA;MACAQ,QAAA;QACAV,QAAA;QACAE,SAAA,EAAAA,SAAA;MACA;MACAS,eAAA;QACAX,QAAA;QACAa,cAAA,EAAAV,MAAA;MACA;IACA;EACA;EAEAW,OAAA;IACA,GAAAV,UAAA;IAEA;AACA;AACA;IACAW,cAAAT,IAAA;MACA;QAAAU,MAAA;QAAAC;MAAA,SAAAC,EAAA,CAAAV,IAAA,CAAAF,IAAA;MACA,OAAAU,MAAA,IAAAC,MAAA;IACA;IAEA;AACA;AACA;IACA,MAAAE,eAAA;MACA,KAAAD,EAAA,CAAAV,IAAA,CAAAY,MAAA;MACA,SAAAF,EAAA,CAAAV,IAAA,CAAAa,QAAA;QACA;MACA;MAEA;QACA;QACA,MAA