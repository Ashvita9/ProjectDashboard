{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n/**\r\n * Navigation Bar Component (Sidebar)\r\n * Fixed left sidebar navigation with user profile and logout button\r\n * Features:\r\n * - Fixed left sidebar (280px on desktop, 70px on mobile)\r\n * - Project and Dashboard navigation links with active state\r\n * - User profile section with DiceBear avatar\r\n * - Logout functionality with event bus notification\r\n * - Claymorphism design with gradient and glass effect\r\n * - Responsive design for mobile devices\r\n */\n\nimport { mapGetters } from 'vuex';\nimport eventBus from '../../bus/eventBus';\nexport default {\n  name: 'NavBar',\n  /**\r\n   * Computed properties (derived from Vuex store)\r\n   * Updates reactively when user data changes\r\n   */\n  computed: {\n    // Map auth getters from Vuex\n    ...mapGetters('auth', ['getUser']),\n    /**\r\n     * Get user's display name from store or default value\r\n     * @returns {string} User name or 'User' if not available\r\n     */\n    userName() {\n      return this.getUser?.name || 'User';\n    },\n    /**\r\n     * Get user's email from store or empty string\r\n     * @returns {string} User email\r\n     */\n    userEmail() {\n      return this.getUser?.email || '';\n    },\n    /**\r\n     * Get user's avatar URL from store\r\n     * Avatar is a DiceBear SVG URL generated on login\r\n     * @returns {string|null} Avatar URL or null if not available\r\n     */\n    userAvatar() {\n      return this.getUser?.avatar || null;\n    }\n  },\n  methods: {\n    /**\r\n     * Handle user logout\r\n     * 1. Dispatch logout action to clear Vuex auth store\r\n     * 2. Emit notification event via event bus\r\n     * 3. Redirect to login page\r\n     */\n    handleLogout() {\n      // Clear authentication data from Vuex (also clears localStorage)\n      this.$store.dispatch('auth/logout');\n\n      // Show logout confirmation notification\n      eventBus.$emit('show-notification', {\n        message: 'Logged out successfully',\n        variant: 'info'\n      });\n\n      // Redirect to login page\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","eventBus","name","computed","userName","getUser","userEmail","email","userAvatar","avatar","methods","handleLogout","$store","dispatch","$emit","message","variant","$router","push"],"sources":["src/components/Layout/NavBar.vue"],"sourcesContent":["<template>\r\n  <div class=\"sidebar-layout\">\r\n    <!-- Sidebar Navigation -->\r\n    <div class=\"sidebar\">\r\n      <div class=\"sidebar-header\">\r\n        <b-icon icon=\"briefcase\" class=\"sidebar-logo\"></b-icon>\r\n        <h5 class=\"sidebar-title\">Project Manager</h5>\r\n      </div>\r\n\r\n      <div class=\"sidebar-nav\">\r\n        <router-link to=\"/dashboard\" class=\"sidebar-link\" exact-active-class=\"active\">\r\n          <b-icon icon=\"house-door\" class=\"sidebar-icon\"></b-icon>\r\n          <span class=\"sidebar-text\">Dashboard</span>\r\n        </router-link>\r\n\r\n        <router-link to=\"/projects\" class=\"sidebar-link\" exact-active-class=\"active\">\r\n          <b-icon icon=\"folder\" class=\"sidebar-icon\"></b-icon>\r\n          <span class=\"sidebar-text\">Projects</span>\r\n        </router-link>\r\n      </div>\r\n\r\n      <div class=\"sidebar-footer\">\r\n        <div class=\"user-profile\">\r\n          <!-- DiceBear Avatar -->\r\n          <img \r\n            v-if=\"userAvatar\" \r\n            :src=\"userAvatar\" \r\n            alt=\"User Avatar\" \r\n            class=\"user-avatar\"\r\n          />\r\n          <b-icon v-else icon=\"person-circle\" class=\"user-avatar-icon\"></b-icon>\r\n          \r\n          <div class=\"user-info\">\r\n            <div class=\"user-name\">{{ userName }}</div>\r\n            <small class=\"user-email\">{{ userEmail }}</small>\r\n          </div>\r\n        </div>\r\n\r\n        <b-button \r\n          variant=\"outline-light\" \r\n          size=\"sm\" \r\n          block \r\n          class=\"logout-btn\"\r\n          @click=\"handleLogout\"\r\n        >\r\n          <b-icon icon=\"box-arrow-right\"></b-icon>\r\n          Logout\r\n        </b-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n/**\r\n * Navigation Bar Component (Sidebar)\r\n * Fixed left sidebar navigation with user profile and logout button\r\n * Features:\r\n * - Fixed left sidebar (280px on desktop, 70px on mobile)\r\n * - Project and Dashboard navigation links with active state\r\n * - User profile section with DiceBear avatar\r\n * - Logout functionality with event bus notification\r\n * - Claymorphism design with gradient and glass effect\r\n * - Responsive design for mobile devices\r\n */\r\n\r\nimport { mapGetters } from 'vuex'\r\nimport eventBus from '../../bus/eventBus'\r\n\r\nexport default {\r\n  name: 'NavBar',\r\n  \r\n  /**\r\n   * Computed properties (derived from Vuex store)\r\n   * Updates reactively when user data changes\r\n   */\r\n  computed: {\r\n    // Map auth getters from Vuex\r\n    ...mapGetters('auth', ['getUser']),\r\n    \r\n    /**\r\n     * Get user's display name from store or default value\r\n     * @returns {string} User name or 'User' if not available\r\n     */\r\n    userName() {\r\n      return this.getUser?.name || 'User'\r\n    },\r\n    \r\n    /**\r\n     * Get user's email from store or empty string\r\n     * @returns {string} User email\r\n     */\r\n    userEmail() {\r\n      return this.getUser?.email || ''\r\n    },\r\n    \r\n    /**\r\n     * Get user's avatar URL from store\r\n     * Avatar is a DiceBear SVG URL generated on login\r\n     * @returns {string|null} Avatar URL or null if not available\r\n     */\r\n    userAvatar() {\r\n      return this.getUser?.avatar || null\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    /**\r\n     * Handle user logout\r\n     * 1. Dispatch logout action to clear Vuex auth store\r\n     * 2. Emit notification event via event bus\r\n     * 3. Redirect to login page\r\n     */\r\n    handleLogout() {\r\n      // Clear authentication data from Vuex (also clears localStorage)\r\n      this.$store.dispatch('auth/logout')\r\n      \r\n      // Show logout confirmation notification\r\n      eventBus.$emit('show-notification', {\r\n        message: 'Logged out successfully',\r\n        variant: 'info'\r\n      })\r\n      \r\n      // Redirect to login page\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar-layout {\r\n  position: relative;\r\n}\r\n\r\n.sidebar {\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  height: 100vh;\r\n  width: 280px;\r\n  background: linear-gradient(180deg, #D4C5F9 0%, #B8D8F2 100%);\r\n  box-shadow: \r\n    4px 0 16px rgba(212, 197, 249, 0.3),\r\n    inset -1px 0 2px rgba(255, 255, 255, 0.5);\r\n  display: flex;\r\n  flex-direction: column;\r\n  z-index: 1000;\r\n  overflow-y: auto;\r\n}\r\n\r\n.sidebar-header {\r\n  padding: 2rem 1.5rem;\r\n  text-align: center;\r\n  border-bottom: 1px solid rgba(255, 255, 255, 0.3);\r\n  background: rgba(255, 255, 255, 0.1);\r\n  backdrop-filter: blur(10px);\r\n}\r\n\r\n.sidebar-logo {\r\n  font-size: 2.5rem;\r\n  color: #1f2937; /* Dark slate for higher contrast */\r\n  margin-bottom: 0.5rem;\r\n  filter: drop-shadow(2px 2px 4px rgba(0, 0, 0, 0.1));\r\n}\r\n\r\n.sidebar-title {\r\n  color: #1f2937; /* Dark slate for higher contrast */\r\n  font-weight: 700;\r\n  margin: 0;\r\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.sidebar-nav {\r\n  flex: 1;\r\n  padding: 1.5rem 1rem;\r\n}\r\n\r\n.sidebar-link {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 1rem 1.5rem;\r\n  color: #243b53; /* Darker link text */\r\n  text-decoration: none;\r\n  border-radius: 12px;\r\n  margin-bottom: 0.75rem;\r\n  transition: all 0.3s ease;\r\n  background: rgba(255, 255, 255, 0.1);\r\n  backdrop-filter: blur(5px);\r\n  box-shadow: \r\n    2px 2px 8px rgba(212, 197, 249, 0.2),\r\n    inset 1px 1px 2px rgba(255, 255, 255, 0.3);\r\n}\r\n\r\n.sidebar-link:hover {\r\n  background: rgba(255, 255, 255, 0.2);\r\n  transform: translateX(5px);\r\n  box-shadow: \r\n    4px 4px 12px rgba(212, 197, 249, 0.3),\r\n    inset 1px 1px 3px rgba(255, 255, 255, 0.4);\r\n  color: #111827; /* Even darker on hover */\r\n}\r\n\r\n.sidebar-link.active {\r\n  background: rgba(255, 255, 255, 0.3);\r\n  box-shadow: \r\n    inset 2px 2px 8px rgba(184, 216, 242, 0.4),\r\n    2px 2px 12px rgba(255, 255, 255, 0.5);\r\n  font-weight: 600;\r\n  color: #111827; /* Darkest for active state */\r\n}\r\n\r\n.sidebar-icon {\r\n  font-size: 1.25rem;\r\n  margin-right: 1rem;\r\n  color: #1f2937; /* Darker icon color */\r\n}\r\n\r\n.sidebar-text {\r\n  font-size: 1rem;\r\n}\r\n\r\n.sidebar-footer {\r\n  padding: 1.5rem 1rem;\r\n  border-top: 1px solid rgba(255, 255, 255, 0.3);\r\n  background: rgba(255, 255, 255, 0.1);\r\n  backdrop-filter: blur(10px);\r\n}\r\n\r\n.user-profile {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 1rem;\r\n  padding: 1rem;\r\n  background: rgba(255, 255, 255, 0.15);\r\n  border-radius: 12px;\r\n  box-shadow: \r\n    2px 2px 8px rgba(212, 197, 249, 0.2),\r\n    inset 1px 1px 2px rgba(255, 255, 255, 0.3);\r\n}\r\n\r\n.user-avatar {\r\n  width: 48px;\r\n  height: 48px;\r\n  border-radius: 50%;\r\n  margin-right: 1rem;\r\n  border: 3px solid rgba(255, 255, 255, 0.5);\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n  background: white;\r\n}\r\n\r\n.user-avatar-icon {\r\n  font-size: 3rem;\r\n  color: white;\r\n  margin-right: 1rem;\r\n  filter: drop-shadow(2px 2px 4px rgba(0, 0, 0, 0.1));\r\n}\r\n\r\n.user-info {\r\n  flex: 1;\r\n  overflow: hidden;\r\n}\r\n\r\n.user-name {\r\n  color: #1f2937; /* Dark text for name */\r\n  font-weight: 600;\r\n  font-size: 1rem;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.user-email {\r\n  color: rgba(31, 41, 55, 0.8); /* Darker muted email */\r\n  font-size: 0.75rem;\r\n  display: block;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n\r\n.logout-btn {\r\n  background: rgba(255, 255, 255, 0.2);\r\n  border: 1px solid rgba(31, 41, 55, 0.4);\r\n  color: #1f2937; /* Dark button text */\r\n  border-radius: 8px;\r\n  font-weight: 600;\r\n  transition: all 0.3s ease;\r\n  box-shadow: \r\n    2px 2px 6px rgba(212, 197, 249, 0.2),\r\n    inset 1px 1px 2px rgba(255, 255, 255, 0.3);\r\n}\r\n\r\n.logout-btn:hover {\r\n  background: rgba(255, 255, 255, 0.3);\r\n  border-color: rgba(31, 41, 55, 0.5);\r\n  transform: translateY(-2px);\r\n  box-shadow: \r\n    3px 3px 10px rgba(212, 197, 249, 0.3),\r\n    inset 1px 1px 3px rgba(255, 255, 255, 0.4);\r\n  color: #111827;\r\n}\r\n\r\n.logout-btn:active {\r\n  transform: translateY(0);\r\n  box-shadow: \r\n    inset 2px 2px 6px rgba(184, 216, 242, 0.3),\r\n    1px 1px 4px rgba(255, 255, 255, 0.3);\r\n}\r\n\r\n/* Responsive */\r\n@media (max-width: 768px) {\r\n  .sidebar {\r\n    width: 70px;\r\n  }\r\n\r\n  .sidebar-text {\r\n    display: none;\r\n  }\r\n\r\n  .sidebar-header {\r\n    padding: 1rem 0.5rem;\r\n  }\r\n\r\n  .sidebar-title {\r\n    display: none;\r\n  }\r\n\r\n  .sidebar-link {\r\n    justify-content: center;\r\n    padding: 1rem 0.5rem;\r\n  }\r\n\r\n  .sidebar-icon {\r\n    margin-right: 0;\r\n  }\r\n\r\n  .user-info {\r\n    display: none;\r\n  }\r\n\r\n  .user-profile {\r\n    justify-content: center;\r\n    padding: 0.5rem;\r\n  }\r\n\r\n  .user-avatar {\r\n    margin-right: 0;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAsDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,SAAAA,UAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EAEA;AACA;AACA;AACA;EACAC,QAAA;IACA;IACA,GAAAH,UAAA;IAEA;AACA;AACA;AACA;IACAI,SAAA;MACA,YAAAC,OAAA,EAAAH,IAAA;IACA;IAEA;AACA;AACA;AACA;IACAI,UAAA;MACA,YAAAD,OAAA,EAAAE,KAAA;IACA;IAEA;AACA;AACA;AACA;AACA;IACAC,WAAA;MACA,YAAAH,OAAA,EAAAI,MAAA;IACA;EACA;EAEAC,OAAA;IACA;AACA;AACA;AACA;AACA;AACA;IACAC,aAAA;MACA;MACA,KAAAC,MAAA,CAAAC,QAAA;;MAEA;MACAZ,QAAA,CAAAa,KAAA;QACAC,OAAA;QACAC,OAAA;MACA;;MAEA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}