{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { mapGetters } from 'vuex';\nimport TaskFormModal from '../components/Task/TaskFormModal.vue';\nimport TaskTable from '../components/Task/TaskTable.vue';\nimport eventBus from '../bus/eventBus';\nexport default {\n  name: 'TasksView',\n  components: {\n    TaskFormModal,\n    TaskTable\n  },\n  data() {\n    return {\n      selectedTask: null,\n      hasUnsavedChanges: false\n    };\n  },\n  computed: {\n    ...mapGetters('projects', ['getProjectById']),\n    projectId() {\n      return parseInt(this.$route.params.id);\n    },\n    project() {\n      return this.getProjectById(this.projectId);\n    },\n    allTasks() {\n      return this.project?.tasks || [];\n    },\n    todoTasks() {\n      return this.allTasks.filter(task => task.status === 'Todo');\n    },\n    inProgressTasks() {\n      return this.allTasks.filter(task => task.status === 'In Progress');\n    },\n    doneTasks() {\n      return this.allTasks.filter(task => task.status === 'Done');\n    }\n  },\n  watch: {\n    projectId: {\n      immediate: true,\n      handler(newId) {\n        if (newId) {\n          this.$store.dispatch('projects/setActiveProject', newId);\n        }\n      }\n    }\n  },\n  created() {\n    this.$store.dispatch('projects/fetchProjects');\n  },\n  beforeRouteLeave(to, from, next) {\n    if (this.hasUnsavedChanges) {\n      const answer = window.confirm('You have unsaved changes. Do you really want to leave?');\n      if (answer) {\n        next();\n      } else {\n        next(false);\n      }\n    } else {\n      next();\n    }\n  },\n  methods: {\n    goBack() {\n      this.$router.push(`/projects/${this.projectId}`);\n    },\n    openCreateModal() {\n      this.selectedTask = null;\n      this.hasUnsavedChanges = true;\n      this.$bvModal.show('task-form-modal');\n    },\n    openEditModal(task) {\n      this.selectedTask = {\n        ...task\n      };\n      this.hasUnsavedChanges = true;\n      this.$bvModal.show('task-form-modal');\n    },\n    confirmDelete(task) {\n      this.$bvModal.msgBoxConfirm(`Are you sure you want to delete \"${task.title}\"?`, {\n        title: 'Confirm Delete',\n        okVariant: 'danger',\n        okTitle: 'Delete',\n        cancelTitle: 'Cancel',\n        centered: true\n      }).then(value => {\n        if (value) {\n          this.$store.dispatch('projects/deleteTask', {\n            projectId: this.projectId,\n            taskId: task.id\n          });\n          eventBus.$emit('show-notification', {\n            message: 'Task deleted successfully',\n            variant: 'warning'\n          });\n        }\n      });\n    },\n    handleStatusChange(task, newStatus) {\n      const updatedTask = {\n        ...task,\n        status: newStatus\n      };\n      this.$store.dispatch('projects/updateTask', {\n        projectId: this.projectId,\n        task: updatedTask\n      });\n      eventBus.$emit('show-notification', {\n        message: `Task status changed to \"${newStatus}\"`,\n        variant: 'info'\n      });\n    },\n    handleTaskSaved() {\n      this.hasUnsavedChanges = false;\n      this.$store.dispatch('projects/fetchProjects');\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","TaskFormModal","TaskTable","eventBus","name","components","data","selectedTask","hasUnsavedChanges","computed","projectId","parseInt","$route","params","id","project","getProjectById","allTasks","tasks","todoTasks","filter","task","status","inProgressTasks","doneTasks","watch","immediate","handler","newId","$store","dispatch","created","beforeRouteLeave","to","from","next","answer","window","confirm","methods","goBack","$router","push","openCreateModal","$bvModal","show","openEditModal","confirmDelete","msgBoxConfirm","title","okVariant","okTitle","cancelTitle","centered","then","value","taskId","$emit","message","variant","handleStatusChange","newStatus","updatedTask","handleTaskSaved"],"sources":["src/views/TasksView.vue"],"sourcesContent":["<template>\r\n  <div class=\"tasks-view\">\r\n    <b-container fluid class=\"py-4\">\r\n      <b-row v-if=\"project\">\r\n        <b-col md=\"12\">\r\n          <div class=\"d-flex justify-content-between align-items-center mb-4\">\r\n            <div>\r\n              <b-button variant=\"link\" @click=\"goBack\">\r\n                <b-icon icon=\"arrow-left\"></b-icon> Back to Project\r\n              </b-button>\r\n              <h2 class=\"mb-0\">Tasks - {{ project.name }}</h2>\r\n            </div>\r\n            <b-button variant=\"primary\" @click=\"openCreateModal\">\r\n              <b-icon icon=\"plus-circle\"></b-icon> Add Task\r\n            </b-button>\r\n          </div>\r\n        </b-col>\r\n      </b-row>\r\n\r\n      <b-row v-if=\"project\">\r\n        <b-col md=\"12\">\r\n          <b-card class=\"shadow-sm\">\r\n            <b-tabs content-class=\"mt-3\">\r\n              <b-tab title=\"All Tasks\" active>\r\n                <task-table\r\n                  :tasks=\"allTasks\"\r\n                  @edit=\"openEditModal\"\r\n                  @delete=\"confirmDelete\"\r\n                  @status-change=\"handleStatusChange\"\r\n                />\r\n              </b-tab>\r\n              <b-tab title=\"Todo\">\r\n                <task-table\r\n                  :tasks=\"todoTasks\"\r\n                  @edit=\"openEditModal\"\r\n                  @delete=\"confirmDelete\"\r\n                  @status-change=\"handleStatusChange\"\r\n                />\r\n              </b-tab>\r\n              <b-tab title=\"In Progress\">\r\n                <task-table\r\n                  :tasks=\"inProgressTasks\"\r\n                  @edit=\"openEditModal\"\r\n                  @delete=\"confirmDelete\"\r\n                  @status-change=\"handleStatusChange\"\r\n                />\r\n              </b-tab>\r\n              <b-tab title=\"Done\">\r\n                <task-table\r\n                  :tasks=\"doneTasks\"\r\n                  @edit=\"openEditModal\"\r\n                  @delete=\"confirmDelete\"\r\n                  @status-change=\"handleStatusChange\"\r\n                />\r\n              </b-tab>\r\n            </b-tabs>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n\r\n    <task-form-modal\r\n      modal-id=\"task-form-modal\"\r\n      :task=\"selectedTask\"\r\n      :project-id=\"projectId\"\r\n      @task-saved=\"handleTaskSaved\"\r\n      @modal-closed=\"selectedTask = null\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\nimport TaskFormModal from '../components/Task/TaskFormModal.vue'\r\nimport TaskTable from '../components/Task/TaskTable.vue'\r\nimport eventBus from '../bus/eventBus'\r\n\r\nexport default {\r\n  name: 'TasksView',\r\n  components: {\r\n    TaskFormModal,\r\n    TaskTable\r\n  },\r\n  data() {\r\n    return {\r\n      selectedTask: null,\r\n      hasUnsavedChanges: false\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters('projects', ['getProjectById']),\r\n    projectId() {\r\n      return parseInt(this.$route.params.id)\r\n    },\r\n    project() {\r\n      return this.getProjectById(this.projectId)\r\n    },\r\n    allTasks() {\r\n      return this.project?.tasks || []\r\n    },\r\n    todoTasks() {\r\n      return this.allTasks.filter(task => task.status === 'Todo')\r\n    },\r\n    inProgressTasks() {\r\n      return this.allTasks.filter(task => task.status === 'In Progress')\r\n    },\r\n    doneTasks() {\r\n      return this.allTasks.filter(task => task.status === 'Done')\r\n    }\r\n  },\r\n  watch: {\r\n    projectId: {\r\n      immediate: true,\r\n      handler(newId) {\r\n        if (newId) {\r\n          this.$store.dispatch('projects/setActiveProject', newId)\r\n        }\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.$store.dispatch('projects/fetchProjects')\r\n  },\r\n  beforeRouteLeave(to, from, next) {\r\n    if (this.hasUnsavedChanges) {\r\n      const answer = window.confirm('You have unsaved changes. Do you really want to leave?')\r\n      if (answer) {\r\n        next()\r\n      } else {\r\n        next(false)\r\n      }\r\n    } else {\r\n      next()\r\n    }\r\n  },\r\n  methods: {\r\n    goBack() {\r\n      this.$router.push(`/projects/${this.projectId}`)\r\n    },\r\n    openCreateModal() {\r\n      this.selectedTask = null\r\n      this.hasUnsavedChanges = true\r\n      this.$bvModal.show('task-form-modal')\r\n    },\r\n    openEditModal(task) {\r\n      this.selectedTask = { ...task }\r\n      this.hasUnsavedChanges = true\r\n      this.$bvModal.show('task-form-modal')\r\n    },\r\n    confirmDelete(task) {\r\n      this.$bvModal.msgBoxConfirm(\r\n        `Are you sure you want to delete \"${task.title}\"?`,\r\n        {\r\n          title: 'Confirm Delete',\r\n          okVariant: 'danger',\r\n          okTitle: 'Delete',\r\n          cancelTitle: 'Cancel',\r\n          centered: true\r\n        }\r\n      ).then(value => {\r\n        if (value) {\r\n          this.$store.dispatch('projects/deleteTask', {\r\n            projectId: this.projectId,\r\n            taskId: task.id\r\n          })\r\n          eventBus.$emit('show-notification', {\r\n            message: 'Task deleted successfully',\r\n            variant: 'warning'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleStatusChange(task, newStatus) {\r\n      const updatedTask = { ...task, status: newStatus }\r\n      this.$store.dispatch('projects/updateTask', {\r\n        projectId: this.projectId,\r\n        task: updatedTask\r\n      })\r\n      eventBus.$emit('show-notification', {\r\n        message: `Task status changed to \"${newStatus}\"`,\r\n        variant: 'info'\r\n      })\r\n    },\r\n    handleTaskSaved() {\r\n      this.hasUnsavedChanges = false\r\n      this.$store.dispatch('projects/fetchProjects')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.tasks-view {\r\n  background-color: #f8f9fa;\r\n  min-height: calc(100vh - 56px);\r\n}\r\n</style>\r\n"],"mappings":";;;AAwEA,SAAAA,UAAA;AACA,OAAAC,aAAA;AACA,OAAAC,SAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ,aAAA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,YAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAT,UAAA;IACAU,UAAA;MACA,OAAAC,QAAA,MAAAC,MAAA,CAAAC,MAAA,CAAAC,EAAA;IACA;IACAC,QAAA;MACA,YAAAC,cAAA,MAAAN,SAAA;IACA;IACAO,SAAA;MACA,YAAAF,OAAA,EAAAG,KAAA;IACA;IACAC,UAAA;MACA,YAAAF,QAAA,CAAAG,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,MAAA;IACA;IACAC,gBAAA;MACA,YAAAN,QAAA,CAAAG,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,MAAA;IACA;IACAE,UAAA;MACA,YAAAP,QAAA,CAAAG,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,MAAA;IACA;EACA;EACAG,KAAA;IACAf,SAAA;MACAgB,SAAA;MACAC,QAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,MAAA,CAAAC,QAAA,8BAAAF,KAAA;QACA;MACA;IACA;EACA;EACAG,QAAA;IACA,KAAAF,MAAA,CAAAC,QAAA;EACA;EACAE,iBAAAC,EAAA,EAAAC,IAAA,EAAAC,IAAA;IACA,SAAA3B,iBAAA;MACA,MAAA4B,MAAA,GAAAC,MAAA,CAAAC,OAAA;MACA,IAAAF,MAAA;QACAD,IAAA;MACA;QACAA,IAAA;MACA;IACA;MACAA,IAAA;IACA;EACA;EACAI,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,mBAAAhC,SAAA;IACA;IACAiC,gBAAA;MACA,KAAApC,YAAA;MACA,KAAAC,iBAAA;MACA,KAAAoC,QAAA,CAAAC,IAAA;IACA;IACAC,cAAAzB,IAAA;MACA,KAAAd,YAAA;QAAA,GAAAc;MAAA;MACA,KAAAb,iBAAA;MACA,KAAAoC,QAAA,CAAAC,IAAA;IACA;IACAE,cAAA1B,IAAA;MACA,KAAAuB,QAAA,CAAAI,aAAA,CACA,oCAAA3B,IAAA,CAAA4B,KAAA,MACA;QACAA,KAAA;QACAC,SAAA;QACAC,OAAA;QACAC,WAAA;QACAC,QAAA;MACA,CACA,EAAAC,IAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAA1B,MAAA,CAAAC,QAAA;YACApB,SAAA,OAAAA,SAAA;YACA8C,MAAA,EAAAnC,IAAA,CAAAP;UACA;UACAX,QAAA,CAAAsD,KAAA;YACAC,OAAA;YACAC,OAAA;UACA;QACA;MACA;IACA;IACAC,mBAAAvC,IAAA,EAAAwC,SAAA;MACA,MAAAC,WAAA;QAAA,GAAAzC,IAAA;QAAAC,MAAA,EAAAuC;MAAA;MACA,KAAAhC,MAAA,CAAAC,QAAA;QACApB,SAAA,OAAAA,SAAA;QACAW,IAAA,EAAAyC;MACA;MACA3D,QAAA,CAAAsD,KAAA;QACAC,OAAA,6BAAAG,SAAA;QACAF,OAAA;MACA;IACA;IACAI,gBAAA;MACA,KAAAvD,iBAAA;MACA,KAAAqB,MAAA,CAAAC,QAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{}