{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { required, email, minLength } from 'vuelidate/lib/validators';\nimport apiService from '../services/apiService';\nimport eventBus from '../bus/eventBus';\nexport default {\n  name: 'Login',\n  data() {\n    return {\n      form: {\n        email: '',\n        password: ''\n      },\n      loading: false\n    };\n  },\n  validations: {\n    form: {\n      email: {\n        required,\n        email\n      },\n      password: {\n        required,\n        minLength: minLength(6)\n      }\n    }\n  },\n  methods: {\n    validateState(name) {\n      const {\n        $dirty,\n        $error\n      } = this.$v.form[name];\n      return $dirty ? !$error : null;\n    },\n    async handleLogin() {\n      this.$v.$touch();\n      if (this.$v.$invalid) {\n        return;\n      }\n      this.loading = true;\n      try {\n        const response = await apiService.login(this.form);\n        const {\n          token,\n          user\n        } = response.data;\n        await this.$store.dispatch('auth/login', {\n          token,\n          user\n        });\n        eventBus.$emit('show-notification', {\n          message: `Welcome back, ${user.name}!`,\n          variant: 'success'\n        });\n        this.$router.push('/dashboard');\n      } catch (error) {\n        eventBus.$emit('show-notification', {\n          message: 'Login failed. Please try again.',\n          variant: 'danger'\n        });\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["required","email","minLength","apiService","eventBus","name","data","form","password","loading","validations","methods","validateState","$dirty","$error","$v","handleLogin","$touch","$invalid","response","login","token","user","$store","dispatch","$emit","message","variant","$router","push","error"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <b-container>\r\n      <b-row class=\"justify-content-center align-items-center min-vh-100\">\r\n        <b-col md=\"6\" lg=\"5\">\r\n          <b-card class=\"shadow-lg\">\r\n            <template #header>\r\n              <h3 class=\"text-center mb-0\">Project Management Portal</h3>\r\n            </template>\r\n\r\n            <b-form @submit.prevent=\"handleLogin\">\r\n              <b-form-group label=\"Email\" label-for=\"email\">\r\n                <b-form-input\r\n                  id=\"email\"\r\n                  v-model=\"form.email\"\r\n                  type=\"email\"\r\n                  placeholder=\"Enter your email\"\r\n                  :state=\"validateState('email')\"\r\n                  @blur=\"$v.form.email.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback v-if=\"!$v.form.email.required\">\r\n                  Email is required\r\n                </b-form-invalid-feedback>\r\n                <b-form-invalid-feedback v-if=\"!$v.form.email.email\">\r\n                  Please enter a valid email\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <b-form-group label=\"Password\" label-for=\"password\">\r\n                <b-form-input\r\n                  id=\"password\"\r\n                  v-model=\"form.password\"\r\n                  type=\"password\"\r\n                  placeholder=\"Enter your password\"\r\n                  :state=\"validateState('password')\"\r\n                  @blur=\"$v.form.password.$touch()\"\r\n                ></b-form-input>\r\n                <b-form-invalid-feedback v-if=\"!$v.form.password.required\">\r\n                  Password is required\r\n                </b-form-invalid-feedback>\r\n                <b-form-invalid-feedback v-if=\"!$v.form.password.minLength\">\r\n                  Password must be at least 6 characters\r\n                </b-form-invalid-feedback>\r\n              </b-form-group>\r\n\r\n              <b-button\r\n                type=\"submit\"\r\n                variant=\"primary\"\r\n                block\r\n                :disabled=\"$v.$invalid || loading\"\r\n              >\r\n                <b-spinner small v-if=\"loading\"></b-spinner>\r\n                {{ loading ? 'Logging in...' : 'Login' }}\r\n              </b-button>\r\n            </b-form>\r\n\r\n            <div class=\"mt-3 text-center text-muted\">\r\n              <small>Demo: Use any email and password (min 6 chars)</small>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { required, email, minLength } from 'vuelidate/lib/validators'\r\nimport apiService from '../services/apiService'\r\nimport eventBus from '../bus/eventBus'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data() {\r\n    return {\r\n      form: {\r\n        email: '',\r\n        password: ''\r\n      },\r\n      loading: false\r\n    }\r\n  },\r\n  validations: {\r\n    form: {\r\n      email: {\r\n        required,\r\n        email\r\n      },\r\n      password: {\r\n        required,\r\n        minLength: minLength(6)\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    validateState(name) {\r\n      const { $dirty, $error } = this.$v.form[name]\r\n      return $dirty ? !$error : null\r\n    },\r\n    async handleLogin() {\r\n      this.$v.$touch()\r\n      if (this.$v.$invalid) {\r\n        return\r\n      }\r\n\r\n      this.loading = true\r\n      try {\r\n        const response = await apiService.login(this.form)\r\n        const { token, user } = response.data\r\n\r\n        await this.$store.dispatch('auth/login', { token, user })\r\n        \r\n        eventBus.$emit('show-notification', {\r\n          message: `Welcome back, ${user.name}!`,\r\n          variant: 'success'\r\n        })\r\n\r\n        this.$router.push('/dashboard')\r\n      } catch (error) {\r\n        eventBus.$emit('show-notification', {\r\n          message: 'Login failed. Please try again.',\r\n          variant: 'danger'\r\n        })\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  /* Claymorphism gradient background with pastel colors */\r\n  background: linear-gradient(135deg, #D4C5F9 0%, #FFD1DC 50%, #B8D8F2 100%);\r\n  min-height: 100vh;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n/* Animated background elements for depth */\r\n.login-container::before {\r\n  content: '';\r\n  position: absolute;\r\n  width: 500px;\r\n  height: 500px;\r\n  background: radial-gradient(circle, rgba(212, 197, 249, 0.4) 0%, transparent 70%);\r\n  border-radius: 50%;\r\n  top: -250px;\r\n  left: -250px;\r\n  animation: float 20s ease-in-out infinite;\r\n}\r\n\r\n.login-container::after {\r\n  content: '';\r\n  position: absolute;\r\n  width: 400px;\r\n  height: 400px;\r\n  background: radial-gradient(circle, rgba(184, 216, 242, 0.3) 0%, transparent 70%);\r\n  border-radius: 50%;\r\n  bottom: -200px;\r\n  right: -200px;\r\n  animation: float 15s ease-in-out infinite reverse;\r\n}\r\n\r\n@keyframes float {\r\n  0%, 100% {\r\n    transform: translate(0, 0) scale(1);\r\n  }\r\n  50% {\r\n    transform: translate(50px, 50px) scale(1.1);\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAmEA,SAAAA,QAAA,EAAAC,KAAA,EAAAC,SAAA;AACA,OAAAC,UAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAN,KAAA;QACAO,QAAA;MACA;MACAC,OAAA;IACA;EACA;EACAC,WAAA;IACAH,IAAA;MACAN,KAAA;QACAD,QAAA;QACAC;MACA;MACAO,QAAA;QACAR,QAAA;QACAE,SAAA,EAAAA,SAAA;MACA;IACA;EACA;EACAS,OAAA;IACAC,cAAAP,IAAA;MACA;QAAAQ,MAAA;QAAAC;MAAA,SAAAC,EAAA,CAAAR,IAAA,CAAAF,IAAA;MACA,OAAAQ,MAAA,IAAAC,MAAA;IACA;IACA,MAAAE,YAAA;MACA,KAAAD,EAAA,CAAAE,MAAA;MACA,SAAAF,EAAA,CAAAG,QAAA;QACA;MACA;MAEA,KAAAT,OAAA;MACA;QACA,MAAAU,QAAA,SAAAhB,UAAA,CAAAiB,KAAA,MAAAb,IAAA;QACA;UAAAc,KAAA;UAAAC;QAAA,IAAAH,QAAA,CAAAb,IAAA;QAEA,WAAAiB,MAAA,CAAAC,QAAA;UAAAH,KAAA;UAAAC;QAAA;QAEAlB,QAAA,CAAAqB,KAAA;UACAC,OAAA,mBAAAJ,IAAA,CAAAjB,IAAA;UACAsB,OAAA;QACA;QAEA,KAAAC,OAAA,CAAAC,IAAA;MACA,SAAAC,KAAA;QACA1B,QAAA,CAAAqB,KAAA;UACAC,OAAA;UACAC,OAAA;QACA;MACA;QACA,KAAAlB,OAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}