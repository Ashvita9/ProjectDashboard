ss=\"flex-grow-1\">\r\n        <h5 class=\"mb-2\">{{ project.name }}</h5>\r\n        <p class=\"text-muted mb-2\">{{ project.description }}</p>\r\n        <div class=\"d-flex align-items-center\">\r\n          <b-badge :variant=\"statusVariant\" class=\"mr-2\">\r\n            {{ project.status }}\r\n          </b-badge>\r\n          <small class=\"text-muted\">\r\n            {{ taskSummary }}\r\n          </small>\r\n        </div>\r\n      </div>\r\n      <div class=\"ml-3\">\r\n        <b-dropdown right variant=\"link\" no-caret>\r\n          <template #button-content>\r\n            <b-icon icon=\"three-dots-vertical\"></b-icon>\r\n          </template>\r\n          <b-dropdown-item @click.stop=\"$emit('edit', project)\">\r\n            Edit\r\n          </b-dropdown-item>\r\n          <b-dropdown-item @click.stop=\"$emit('delete', project)\">\r\n            Delete\r\n          </b-dropdown-item>\r\n        </b-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <template #footer v-if=\"project.startDate || project.endDate\">\r\n      <small class=\"text-muted\">\r\n        <b-icon icon=\"calendar\"></b-icon>\r\n        {{ formatDateRange }}\r\n      </small>\r\n    </template>\r\n  </b-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ProjectCard',\r\n  props: {\r\n    project: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    isActive: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  computed: {\r\n    statusVariant() {\r\n      const variants = {\r\n        'Planning': 'info',\r\n        'In Progress': 'warning',\r\n        'Completed': 'success',\r\n        'On Hold': 'secondary'\r\n      }\r\n      return variants[this.project.status] || 'primary'\r\n    },\r\n    taskSummary() {\r\n      const tasks = this.project.tasks || []\r\n      const total = tasks.length\r\n      const completed = tasks.filter(t => t.status === 'Done').length\r\n      return total > 0 ? `${completed}/${total} tasks completed` : 'No tasks yet'\r\n    },\r\n    formatDateRange() {\r\n      const start = this.project.startDate ? new Date(this.project.startDate).toLocaleDateString() : ''\r\n      const end = this.project.endDate ? new Date(this.project.endDate).toLocaleDateString() : ''\r\n      if (start && end) return `${start} - ${end}`\r\n      if (start) return `From ${start}`\r\n      if (end) return `Until ${end}`\r\n      return ''\r\n    }\r\n  },\r\n  methods: {\r\n    handleClick() {\r\n      this.$emit('click', this.project)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.project-card {\r\n  cursor: pointer;\r\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.project-card::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(135deg, rgba(212, 197, 249, 0.1) 0%, rgba(184, 216, 242, 0.1) 100%);\r\n  opacity: 0;\r\n  transition: opacity 0.3s ease;\r\n  pointer-events: none;\r\n}\r\n\r\n.project-card:hover::before {\r\n  opacity: 1;\r\n}\r\n\r\n.project-card:hover {\r\n  transform: translateY(-8px) scale(1.02);\r\n  box-shadow: 8px 8px 16px rgba(107, 91, 149, 0.18),\r\n              -8px -8px 16px rgba(255, 255, 255, 0.95) !important;\r\n}\r\n\r\n.project-card.border-primary {\r\n  border-width: 2px;\r\n  box-shadow: 0 0 0 3px rgba(212, 197, 249, 0.3),\r\n              4px 4px 8px rgba(107, 91, 149, 0.12),\r\n              -4px -4px 8px rgba(255, 255, 255, 0.9) !important;\r\n}\r\n</style>\r\n"],"mappings":";;AA4CA;EACAA,IAAA;EACAC,KAAA;IACAC,OAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,QAAA;IACA;IACAC,QAAA;MACAH,IAAA,EAAAI,OAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACAC,cAAA;MACA,MAAAC,QAAA;QACA;QACA;QACA;QACA;MACA;MACA,OAAAA,QAAA,MAAAT,OAAA,CAAAU,MAAA;IACA;IACAC,YAAA;MACA,MAAAC,KAAA,QAAAZ,OAAA,CAAAY,KAAA;MACA,MAAAC,KAAA,GAAAD,KAAA,CAAAE,MAAA;MACA,MAAAC,SAAA,GAAAH,KAAA,CAAAI,MAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAP,MAAA,aAAAI,MAAA;MACA,O