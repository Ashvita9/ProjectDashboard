lProjects', 'projectCount', 'completedTaskCount', 'activeProject']),\r\n    recentProjects() {\r\n      return this.allProjects.slice(0, 5)\r\n    },\r\n    allTasks() {\r\n      const tasks = []\r\n      this.allProjects.forEach(project => {\r\n        if (project.tasks) {\r\n          tasks.push(...project.tasks)\r\n        }\r\n      })\r\n      return tasks\r\n    },\r\n    totalTaskCount() {\r\n      return this.allTasks.length\r\n    },\r\n    inProgressTaskCount() {\r\n      return this.allTasks.filter(task => task.status === 'In Progress').length\r\n    }\r\n  },\r\n  created() {\r\n    this.$store.dispatch('projects/fetchProjects')\r\n  },\r\n  methods: {\r\n    getStatusVariant(status) {\r\n      const variants = {\r\n        'Planning': 'info',\r\n        'In Progress': 'warning',\r\n        'Completed': 'success',\r\n        'On Hold': 'secondary'\r\n      }\r\n      return variants[status] || 'primary'\r\n    },\r\n    getProjectTaskCount(project) {\r\n      return project.tasks ? project.tasks.length : 0\r\n    },\r\n    getProjectCompletedCount(project) {\r\n      return project.tasks ? project.tasks.filter(t => t.status === 'Done').length : 0\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard {\r\n  background: linear-gradient(135deg, #F5F0F6 0%, #E8DFF5 100%);\r\n  min-height: 100vh;\r\n  padding-bottom: 2rem;\r\n}\r\n\r\n/* Stats Cards */\r\n.stat-card {\r\n  animation: fadeInUp 0.6s ease-out;\r\n  border-radius: 16px;\r\n  border: none;\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\n.stat-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 8px 24px rgba(212, 197, 249, 0.3);\r\n}\r\n\r\n.stat-icon {\r\n  width: 60px;\r\n  height: 60px;\r\n  margin: 0 auto 1rem;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border-radius: 50%;\r\n  background: linear-gradient(135deg, #D4C5F9, #B8D8F2);\r\n  color: white;\r\n  box-shadow: \r\n    4px 4px 12px rgba(212, 197, 249, 0.3),\r\n    -2px -2px 8px rgba(255, 255, 255, 0.8);\r\n}\r\n\r\n.stat-icon.success {\r\n  background: linear-gradient(135deg, #C1F0DC, #B8D8F2);\r\n}\r\n\r\n.stat-icon.warning {\r\n  background: linear-gradient(135deg, #FFE5D4, #FFD1DC);\r\n}\r\n\r\n.stat-icon.info {\r\n  background: linear-gradient(135deg, #B8D8F2, #E6E6FA);\r\n}\r\n\r\n.stat-number {\r\n  font-size: 2.5rem;\r\n  font-weight: 700;\r\n  margin: 0;\r\n  background: linear-gradient(135deg, #D4C5F9, #B8D8F2);\r\n  background-clip: text;\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n}\r\n\r\n/* Chart Cards */\r\n.chart-card {\r\n  border-radius: 16px;\r\n  border: none;\r\n  animation: fadeInUp 0.7s ease-out;\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 100%;\r\n}\r\n\r\n.chart-container {\r\n  height: 250px;\r\n  width: 100%;\r\n  padding: 0.5rem;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  position: relative;\r\n}\r\n\r\n/* Project Items */\r\n.project-item {\r\n  cursor: pointer;\r\n  transition: all 0.3s ease;\r\n  border-radius: 8px;\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.project-item:hover {\r\n  background: rgba(212, 197, 249, 0.1);\r\n  transform: translateX(5px);\r\n}\r\n\r\n/* Custom spacing between cards (increase gutters) */\r\n.stat-row > .col-md-3,\r\n.stat-row > [class*=\"col-\"] {\r\n  padding-left: 16px;\r\n  padding-right: 16px;\r\n}\r\n\r\n.chart-row > .col-md-4,\r\n.chart-row > [class*=\"col-\"] {\r\n  padding-left: 16px;\r\n  padding-right: 16px;\r\n}\r\n\r\n@keyframes fadeInUp {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;AAqIA,SAAAA,UAAA;AACA,OAAAC,eAAA;AACA,OAAAC,iBAAA;AACA,OAAAC,oBAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ,eAAA;IACAC,iBAAA;IACAC;EACA;EACAG,QAAA;IACA,GAAAN,UAAA;IACAO,eAAA;MACA,YAAAC,WAAA,CAAAC,KAAA;IACA;IACAC,SAAA;MACA,MAAAC,KAAA;MACA,KAA