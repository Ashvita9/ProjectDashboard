forEach","project","push","totalTaskCount","length","inProgressTaskCount","filter","task","status","created","$store","dispatch","methods","getStatusVariant","variants","getProjectTaskCount","getProjectCompletedCount","t"],"sources":["src/views/Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <b-container fluid class=\"py-4\">\r\n      <b-row>\r\n        <b-col md=\"12\">\r\n          <h2 class=\"mb-4\">Dashboard Overview</h2>\r\n        </b-col>\r\n      </b-row>\r\n\r\n      <!-- Stats Cards -->\r\n      <b-row class=\"stat-row\">\r\n        <b-col md=\"3\">\r\n          <b-card class=\"stat-card text-center shadow-sm mb-4\">\r\n            <div class=\"stat-icon\">\r\n              <b-icon icon=\"folder\" font-scale=\"2\"></b-icon>\r\n            </div>\r\n            <h1 class=\"stat-number\">{{ projectCount }}</h1>\r\n            <p class=\"text-muted\">Total Projects</p>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col md=\"3\">\r\n          <b-card class=\"stat-card text-center shadow-sm mb-4\">\r\n            <div class=\"stat-icon success\">\r\n              <b-icon icon=\"check-circle\" font-scale=\"2\"></b-icon>\r\n            </div>\r\n            <h1 class=\"stat-number\">{{ completedTaskCount }}</h1>\r\n            <p class=\"text-muted\">Completed Tasks</p>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col md=\"3\">\r\n          <b-card class=\"stat-card text-center shadow-sm mb-4\">\r\n            <div class=\"stat-icon warning\">\r\n              <b-icon icon=\"clock-history\" font-scale=\"2\"></b-icon>\r\n            </div>\r\n            <h1 class=\"stat-number\">{{ inProgressTaskCount }}</h1>\r\n            <p class=\"text-muted\">In Progress</p>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col md=\"3\">\r\n          <b-card class=\"stat-card text-center shadow-sm mb-4\">\r\n            <div class=\"stat-icon info\">\r\n              <b-icon icon=\"list-task\" font-scale=\"2\"></b-icon>\r\n            </div>\r\n            <h1 class=\"stat-number\">{{ totalTaskCount }}</h1>\r\n            <p class=\"text-muted\">Total Tasks</p>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n\r\n      <!-- Charts Row -->\r\n      <b-row class=\"chart-row\">\r\n        <b-col md=\"4\">\r\n          <b-card class=\"chart-card shadow-sm mb-4\">\r\n            <template #header>\r\n              <h5 class=\"mb-0\">Task Status Distribution</h5>\r\n            </template>\r\n            <div class=\"chart-container\">\r\n              <task-status-chart :tasks=\"allTasks\" />\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-card class=\"chart-card shadow-sm mb-4\">\r\n            <template #header>\r\n              <h5 class=\"mb-0\">Task Priority Breakdown</h5>\r\n            </template>\r\n            <div class=\"chart-container\">\r\n              <task-priority-chart :tasks=\"allTasks\" />\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-card class=\"chart-card shadow-sm mb-4\">\r\n            <template #header>\r\n              <h5 class=\"mb-0\">Project Progress</h5>\r\n            </template>\r\n            <div class=\"chart-container\">\r\n              <project-progress-chart :projects=\"allProjects\" />\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n\r\n      <!-- Recent Projects -->\r\n      <b-row>\r\n        <b-col md=\"12\">\r\n          <b-card class=\"shadow-sm\">\r\n            <template #header>\r\n              <div class=\"d-flex justify-content-between align-items-center\">\r\n                <h5 class=\"mb-0\">Recent Projects</h5>\r\n                <b-button variant=\"primary\" size=\"sm\" @click=\"$router.push('/projects')\">\r\n                  View All\r\n                </b-button>\r\n              </div>\r\n            </template>\r\n\r\n            <div v-if